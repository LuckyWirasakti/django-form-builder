{
  "name": "django form builder",
  "description": "A simple project to get started with Django Form Builder on Heroku",
  "repository": "https://github.com/LuckyWirasakti/django-form-builder",
  "keywords": [
    "python",
    "django",
    "template",
    "form",
    "builder"
  ],
  "env": {
    "SECRET_KEY": {
      "generator": "secret"
    },
    "DISABLE_COLLECTSTATIC": {
      "value": "1"
    },
    "DEBUG": {
      "value": "1"
    },
    "ALLOWED_HOSTS": {
      "value": "*"
    }
  },
  "scripts": {
    "postdeploy": "python manage.py migrate --noinput"
  }
}